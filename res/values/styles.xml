<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.

    -->
    <style name="LightTheme" parent="android:Theme.Holo.Light.NoActionBar">
    </style>

    <style name="DarkTheme" parent="android:Theme.Holo.NoActionBar">
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="LightTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="grayBtn">
        <item name="android:background">@drawable/graybtn_selector</item>
    </style>

    <style name="whiteBtn">
        <item name="android:background">@drawable/whitebtn_selector</item>
    </style>

    <style name="transBtn">
        <item name="android:background">@drawable/transbtn_selector</item>
    </style>

</resources>